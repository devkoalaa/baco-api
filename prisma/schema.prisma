generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Item {
  id        String    @id @default(uuid()) @db.Uuid
  name      String
  quantity  String    @db.VarChar(255)
  image     String?   @db.VarChar(255)
  createdAt DateTime  @default(now()) @db.Date
  deletedAt DateTime? @db.Date
}
