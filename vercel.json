{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["baco-api.json", "prisma/schema.prisma"]
      }
    }
  ],
  "build": {
    "commands": {
      "build": "prisma generate && tsc"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ]
}
